    // Html
    {
      "name": "Html - Meta Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },

    // CSS File
    {
      "name": "CSS - Meta Selector",
      "scope": ["source.css meta.selector.css"],
      "settings": {
        "foreground": "#C792EA"
      }
    },

    {
      "name": "CSS - Meta Selector",
      "scope": ["source.css punctuation"],
